<%= "#{@posts.size.to_s} active posts found." %>

<div class="container">
  <div class="row">
    <div class="col-8">
      <% @posts.each do |post| %>
        <div class="insta-post">
          <div class='post-meta'><%= post.account.username %></div>
          <%= cl_image_tag post.photo, class: 'img img-fluid' %>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-4">
    <div class="account-summary">
      <p><strong><%= current_account.username %></strong> <br>
      <%= current_account.full_name %></p>
    </div>
    <div class="follower-suggestions">

    </div>
  </div>
</div>
